{
	"version": "3",
	"templates": [
		{
			"id": 1,
			"type": 3,
			"title": "Traefik",
			"description": "A modern HTTP reverse proxy and load balancer that makes deploying microservices easy.",
			"logo": "https://apps.univrs.cloud/images/traefik.png",
			"categories": [
				"Networking"
			],
			"name": "traefik",
			"env": [
				{
					"type": "text",
					"label": "Domain",
					"name": "DOMAIN",
					"default": ""
				},
				{
					"type": "email",
					"label": "Email address (Let's Encrypt)",
					"name": "EMAIL",
					"default": ""
				}
			],
			"platform": "linux",
			"repository": {
				"url": "https://github.com/univrs-cloud/virgo-apps/",
				"stackfile": "nginx-proxy-manager/docker-compose.yml"
			}
		},
		{
			"id": 2,
			"type": 3,
			"title": "Authelia",
			"description": "The Single Sign-On Multi-Factor portal for web apps.",
			"logo": "https://apps.univrs.cloud/images/authelia.png",
			"categories": [
				"System"
			],
			"name": "authelia",
			"env": [
				{
					"type": "text",
					"label": "Domain",
					"name": "DOMAIN",
					"default": ""
				},
				{
					"type": "text",
					"label": "SMTP server address",
					"name": "SMTP_ADDRESS",
					"default": ""
				},
				{
					"type": "text",
					"label": "SMTP server username",
					"name": "SMTP_USERNAME",
					"default": ""
				},
				{
					"type": "password",
					"label": "SMTP server password",
					"name": "SMTP_PASSWORD",
					"default": ""
				},
				{
					"type": "email",
					"label": "SMTP sender",
					"name": "SMTP_SENDER",
					"default": ""
				},
				{
					"type": "radio",
					"label": "SMTP TLS",
					"name": "SMTP_DISABLE_REQUIRE_TLS",
					"select": [
						{
							"value": "false",
							"text": "Yes"
						},
						{
							"value": "true",
							"default": true,
							"text": "No"
						}
					]
				},
				{
					"type": "radio",
					"label": "SMTP STARTTLS",
					"name": "SMTP_DISABLE_STARTTLS",
					"select": [
						{
							"value": "false",
							"default": true,
							"text": "Yes"
						},
						{
							"value": "true",
							"text": "No"
						}
					]
				}
			],
			"platform": "linux",
			"repository": {
				"url": "https://github.com/univrs-cloud/virgo-apps/",
				"stackfile": "authelia/docker-compose.yml"
			}
		},
		{
			"id": 3,
			"type": 3,
			"title": "Pi-hole",
			"description": "A network-level advertisement and internet tracker blocking application which acts as a DNS sinkhole. This version has Ubound software installed on it so you don't need to rely on external DNS providers.",
			"logo": "https://apps.univrs.cloud/images/pihole.png",
			"categories": [
				"Networking"
			],
			"env": [
				{
					"type": "text",
					"label": "Domain",
					"name": "DOMAIN",
					"default": "univrs"
				},
				{
					"type": "text",
					"label": "Host IP address",
					"name": "HOST_IP",
					"default": "192.168.100.2"
				},
				{
					"type": "radio",
					"label": "DHCP server enabled",
					"name": "DHCP_ACTIVE",
					"select": [
						{
							"value": "true",
							"default": true,
							"text": "Yes"
						},
						{
							"value": "false",
							"text": "No"
						}
					]
				},
				{
					"type": "text",
					"label": "DHCP from",
					"name": "DHCP_START",
					"default": "192.168.100.100"
				},
				{
					"type": "text",
					"label": "DHCP to",
					"name": "DHCP_END",
					"default": "192.168.100.250"
				},
				{
					"type": "text",
					"label": "Router (gateway) IP address",
					"name": "DHCP_ROUTER",
					"default": "192.168.100.1"
				},
				{
					"type": "password",
					"label": "Password (administrator)",
					"name": "PASSWORD",
					"default": ""
				}
			],
			"name": "pihole",
			"platform": "linux",
			"repository": {
				"url": "https://github.com/univrs-cloud/virgo-apps/",
				"stackfile": "pihole/docker-compose.yml"
			}
		},
		{
			"id": 4,
			"type": 3,
			"title": "WireGuard",
			"description": "An extremely simple yet fast and modern VPN.",
			"logo": "https://apps.univrs.cloud/images/wireguard.png",
			"categories": [
				"Networking"
			],
			"env": [
				{
					"type": "text",
					"label": "Domain",
					"name": "DOMAIN",
					"default": ""
				},
				{
					"type": "text",
					"label": "Password hash (brypt)",
					"name": "PASSWORD_HASH",
					"default": "",
					"description": "Leave blank to access UI without logging in."
				},
				{
					"type": "text",
					"label": "DNS",
					"name": "DEFAULT_DNS",
					"default": "192.168.100.2"
				},
				{
					"type": "text",
					"label": "Default address",
					"name": "DEFAULT_ADDRESS",
					"default": "10.6.0.x"
				},
				{
					"type": "text",
					"label": "Allowed IPs",
					"name": "ALLOWED_IPS",
					"default": "0.0.0.0/0"
				}
			],
			"name": "wg-easy",
			"platform": "linux",
			"repository": {
				"url": "https://github.com/univrs-cloud/virgo-apps/",
				"stackfile": "wireguard-easy/docker-compose.yml"
			}
		},
		{
			"id": 5,
			"type": 3,
			"title": "WeTTY",
			"description": "A terminal in a browser.",
			"logo": "https://apps.univrs.cloud/images/terminal.png",
			"categories": [
				"Networking"
			],
			"env": [
				{
					"type": "text",
					"label": "Domain",
					"name": "DOMAIN",
					"default": ""
				},
				{
					"type": "text",
					"label": "Host",
					"name": "SSHHOST",
					"default": "192.168.100.2"
				}
			],
			"name": "wetty",
			"platform": "linux",
			"repository": {
				"url": "https://github.com/univrs-cloud/virgo-apps/",
				"stackfile": "wetty/docker-compose.yml"
			}
		},
		{
			"id": 6,
			"type": 3,
			"title": "qBittorrent",
			"description": "A BitTorrent client.",
			"logo": "https://apps.univrs.cloud/images/qbittorrent.png",
			"categories": [
				"Productivity"
			],
			"name": "qbittorrent",
			"platform": "linux",
			"repository": {
				"url": "https://github.com/univrs-cloud/virgo-apps/",
				"stackfile": "qbittorent/docker-compose.yml"
			}
		},
		{
			"id": 7,
			"type": 3,
			"title": "NextCloud",
			"description": "A file sharing and collaboration platform that allows users to store, access, and share their data from any device or location.",
			"note": "The host of the database is nextcloud-db:3306. The database, database user and database password is `nextcloud`.",
			"logo": "https://apps.univrs.cloud/images/nextcloud.png",
			"categories": [
				"Productivity"
			],
			"env": [
				{
					"type": "text",
					"label": "Domain",
					"name": "DOMAIN",
					"default": ""
				},
				{
					"type": "text",
					"label": "Username (administrator)",
					"name": "NEXTCLOUD_ADMIN_USER",
					"default": "voyager"
				},
				{
					"type": "password",
					"label": "Password (administrator)",
					"name": "NEXTCLOUD_ADMIN_PASSWORD",
					"default": ""
				},
				{
					"type": "text",
					"label": "Trusted domains",
					"name": "NEXTCLOUD_TRUSTED_DOMAINS"
				},
				{
					"type": "text",
					"label": "OnlyOffice JWT secret",
					"name": "ONLYOFFICE_JWT_SECRET"
				}
			],
			"name": "nextcloud",
			"platform": "linux",
			"repository": {
				"url": "https://github.com/univrs-cloud/virgo-apps/",
				"stackfile": "nextcloud/docker-compose.yml"
			}
		},
		{
			"id": 8,
			"type": 3,
			"title": "OneDev",
			"description": "Git hosting, code review, team collaboration, package registry and CI/CD.",
			"logo": "https://apps.univrs.cloud/images/onedev.png",
			"categories": [
				"Productivity"
			],
			"env": [
				{
					"type": "text",
					"label": "Domain",
					"name": "DOMAIN",
					"default": ""
				}
			],
			"name": "onedev",
			"platform": "linux",
			"repository": {
				"url": "https://github.com/univrs-cloud/virgo-apps/",
				"stackfile": "onedev/docker-compose.yml"
			}
		},
		{
			"id": 9,
			"type": 3,
			"title": "DDclient",
			"description": "A client used to update dynamic DNS entries for accounts on Dynamic DNS Network Service Provider.",
			"logo": "https://apps.univrs.cloud/images/ddns-updater.png",
			"categories": [
				"Networking"
			],
			"name": "ddclient",
			"platform": "linux",
			"repository": {
				"url": "https://github.com/univrs-cloud/virgo-apps/",
				"stackfile": "ddclient/docker-compose.yml"
			}
		}
	]
}
