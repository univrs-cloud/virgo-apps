{
	"version":"3",
	"templates":[
		{
			"id":1,
			"type":3,
			"title":"NGINX Proxy Manager",
			"description":"Expose your services easily and securely.",
			"logo":"https://apps.univrs.cloud/images/nginx-proxy-manager.png",
			"categories":["Networking"],
			"name":"nginx-proxy-manager",
			"env":[
				{
					"label":"Email address",
					"name":"ADMIN_EMAIL",
					"default":""
				},
				{
					"label":"Password",
					"name":"ADMIN_PASSWORD",
					"default":"pa$$word"
				}
			],
			"platform":"linux",
			"repository":{
				"url":"https://apps.univrs.cloud/",
				"stackfile":"stack/nginx-proxy-manager.yaml"
			}
		},
		{
			"id":2,
			"type":3,
			"title":"Authelia",
			"description":"The Single Sign-On Multi-Factor portal for web apps.",
			"logo":"https://apps.univrs.cloud/images/authelia.png",
			"categories":["System"],
			"name":"authelia",
			"platform":"linux",
			"repository":{
				"url":"https://apps.univrs.cloud/",
				"stackfile":"stack/authelia.yaml"
			}
		},
{
			"id":3,
			"type":3,
			"title":"Pi-hole",
			"description":"A network-level advertisement and internet tracker blocking application which acts as a DNS sinkhole. This version has Ubound software installed on it so you don't need to rely on external DNS providers.",
			"logo":"https://apps.univrs.cloud/images/pihole.png",
			"categories":["Networking"],
			"env":[
				{
					"label":"Domain name",
					"name":"DOMAIN_NAME",
					"default":"univrs"
				},
				{
					"label":"Host IP address",
					"name":"FTLCONF_LOCAL_IPV4",
					"default":"192.168.100.2"
				},
				{
					"label":"DHCP server enabled",
					"name":"DHCP_ACTIVE",
					"select":[
						{
							"value":"true",
							"default":true,
							"text":"Yes"
						},
						{
							"value":"false",
							"text":"No"
						}
					]
				},
				{
					"label":"DHCP from",
					"name":"DHCP_START",
					"default":"192.168.100.100"
				},
				{
					"label":"DHCP to",
					"name":"DHCP_END",
					"default":"192.168.100.250"
				},
				{
					"label":"Router (gateway) IP address",
					"name":"DHCP_ROUTER",
					"default":"192.168.100.1"
				},
				{
					"label":"Password",
					"name":"PASSWORD",
					"default":"random.1"
				}
			],
			"name":"pihole",
			"platform":"linux",
			"repository":{
				"url":"https://apps.univrs.cloud/",
				"stackfile":"stack/pihole6.yaml"
			}
		},
		{
			"id":5,
			"type":3,
			"title":"WireGuard",
			"description":"An extremely simple yet fast and modern VPN.",
			"logo":"https://apps.univrs.cloud/images/wireguard.png",
			"categories":["Networking"],
			"env":[
				{
					"label":"Host",
					"name":"WG_HOST",
					"default":"vpn.domain.com"
				},
				{
					"label":"Password hash (brypt)",
					"name":"PASSWORD_HASH",
					"default":"",
					"description":"Leave blank to access UI without logging in."
				},
				{
					"label":"DNS",
					"name":"WG_DEFAULT_DNS",
					"default":"192.168.100.2"
				},
				{
					"label":"Default address",
					"name":"WG_DEFAULT_ADDRESS",
					"default":"10.6.0.x"
				},
				{
					"label":"Allowed IPs",
					"name":"WG_ALLOWED_IPS",
					"default":"0.0.0.0/0"
				}
			],
			"name":"wg-easy",
			"platform":"linux",
			"repository":{
				"url":"https://apps.univrs.cloud/",
				"stackfile":"stack/wireguard-easy.yaml"
			}
		},
		{
			"id":6,
			"type":1,
			"title":"WeTTY",
			"description":"A terminal in a browser.",
			"logo":"https://apps.univrs.cloud/images/terminal.png",
			"categories":["Networking"],
			"env":[
				{
					"label":"BASE",
					"name":"BASE",
					"default":"/",
					"preset":true
				},
				{
					"label":"Host",
					"name":"SSHHOST",
					"default":"192.168.100.2"
				},
				{
					"label":"Username",
					"name":"SSHUSER",
					"default":"voyager"
				}
			],
			"command":"--ssh-user=voyager",
			"image":"ghcr.io/butlerx/wetty:latest",
			"name":"wetty",
			"platform":"linux",
			"restart_policy":"unless-stopped",
			"ports":[
				"2222:3000/tcp"
			]
		},
		{
			"id":7,
			"type":1,
			"title":"qBittorrent",
			"description":"A BitTorrent client.",
			"logo":"https://apps.univrs.cloud/images/qbittorrent.png",
			"categories":["Productivity"],
			"env":[
				{
					"label":"PUID",
					"name":"PUID",
					"default":"1000",
					"preset":true
				},
				{
					"label":"PGID",
					"name":"PGID",
					"default":"100",
					"preset":true
				},
				{
					"label":"WEBUI_PORT",
					"name":"WEBUI_PORT",
					"default":"6080",
					"preset":true
				}
			],
			"image":"lscr.io/linuxserver/qbittorrent:latest",
			"name":"qbittorrent",
			"platform":"linux",
			"restart_policy":"unless-stopped",
			"ports":[
				"6080:6080/tcp",
				"6881:6881/tcp",
				"6881:6881/udp"
			],
			"volumes":[
				{
					"bind":"/messier/apps/qbittorrent",
					"container":"/config"
				},
				{
					"bind":"/downloads",
					"container":"/downloads"
				}
			]
		},
		{
			"id":8,
			"type":3,
			"title":"NextCloud",
			"description":"A file sharing and collaboration platform that allows users to store, access, and share their data from any device or location.",
			"note":"The host of the database is nextcloud-db:3306. The database user is nextcloud and the database is nextcloud.",
			"logo":"https://apps.univrs.cloud/images/nextcloud.png",
			"categories":["Productivity"],
			"env":[
				{
					"label":"Username",
					"name":"NEXTCLOUD_ADMIN_USER",
					"default":"voyager"
				},
				{
					"label":"Password",
					"name":"NEXTCLOUD_ADMIN_PASSWORD",
					"default":"pa$$word"
				},
				{
					"label":"Trusted domains",
					"name":"NEXTCLOUD_TRUSTED_DOMAINS"
				},
				{
					"label":"Data folder path",
					"name":"NEXTCLOUD_DATA_DIR",
					"default":"/nextcloud"
				},
				{
					"label":"MySQL password",
					"name":"MYSQL_PASSWORD",
					"default":"nextcloud"
				},
				{
					"label":"MySQL root password",
					"name":"MYSQL_ROOT_PASSWORD",
					"default":"nextcloud"
				},
				{
					"label":"OnlyOffice JWT secret",
					"name":"ONLYOFFICE_JWT_SECRET"
				}
			],
			"name":"nextcloud",
			"platform":"linux",
			"repository":{
				"url":"https://apps.univrs.cloud/",
				"stackfile":"stack/nextcloud.yaml"
			}
		},
		{
			"id":9,
			"type":3,
			"title":"Penpot",
			"description":"A design & prototyping tool for product teams.",
			"logo":"https://apps.univrs.cloud/images/penpot.png",
			"categories":["Productivity"],
			"env":[
				{
					"label":"Public address",
					"name":"PUBLIC_URI",
					"default":"http://ip:5080"
				},
				{
					"label":"PostgreSQL password",
					"name":"POSTGRES_PASSWORD",
					"default":"penpot"
				}
			],
			"name":"penpot",
			"platform":"linux",
			"repository":{
				"url":"https://apps.univrs.cloud/",
				"stackfile":"stack/penpot.yaml"
			}
		},
		{
			"id":10,
			"type":1,
			"title":"OneDev",
			"description":"Git hosting, code review, team collaboration, package registry and CI/CD",
			"logo":"https://apps.univrs.cloud/images/onedev.png",
			"categories":["Productivity"],
			"env":[
				{
					"label":"PUID",
					"name":"PUID",
					"default":"1000",
					"preset":true
				},
				{
					"label":"PGID",
					"name":"PGID",
					"default":"100",
					"preset":true
				}
			],
			"image":"1dev/server:latest",
			"name":"onedev",
			"platform":"linux",
			"restart_policy":"unless-stopped",
			"ports":[
				"6610:6610/tcp",
				"6611:6611/tcp"
			],
			"volumes":[
				{
					"bind":"/messier/apps/onedev",
					"container":"/opt/onedev"
				},
				{
					"bind":"/var/run/docker.sock",
					"container":"/var/run/docker.sock",
					"readonly":true
				}
			]
		},
		{
			"id":11,
			"type":1,
			"title":"Watchtower",
			"description":"Automatically updates your apps to the latest version.",
			"logo":"https://apps.univrs.cloud/images/watchtower.png",
			"categories":["System"],
			"image":"ghcr.io/containrrr/watchtower:latest",
			"command":"--cleanup",
			"name":"watchtower",
			"platform":"linux",
			"restart_policy":"unless-stopped",
			"volumes":[
				{
					"bind":"/var/run/docker.sock",
					"container":"/var/run/docker.sock",
					"readonly":true
				}
			]
		},
		{
			"id":12,
			"type":1,
			"title":"DDclient",
			"description":"A client used to update dynamic DNS entries for accounts on Dynamic DNS Network Service Provider.",
			"logo":"https://apps.univrs.cloud/images/ddns-updater.png",
			"categories":["Networking"],
			"env":[
				{
					"label":"PUID",
					"name":"PUID",
					"default":"1000",
					"preset":true
				},
				{
					"label":"PGID",
					"name":"PGID",
					"default":"100",
					"preset":true
				}
			],
			"image":"lscr.io/linuxserver/ddclient:latest",
			"name":"ddclient",
			"platform":"linux",
			"restart_policy":"unless-stopped",
			"volumes":[
				{
					"bind":"/messier/apps/ddclient",
					"container":"/config"
				}
			]
		}
	]
}
