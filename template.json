{
	"version":"2",
	"templates":[
		{
			"title":"DDclient",
			"description":"A client used to update dynamic DNS entries for accounts on Dynamic DNS Network Service Provider.",
			"logo":"https://raw.githubusercontent.com/univrs-cloud/virgo-apps/main/images/ddns-updater.png",
			"categories":["Networking"],
			"env":[
				{
					"label":"PUID",
					"name":"PUID",
					"default":"1000",
					"preset":true
				},
				{
					"label":"PGID",
					"name":"PGID",
					"default":"100",
					"preset":true
				}
			],
			"image":"lscr.io/linuxserver/ddclient:latest",
			"name":"ddclient",
			"platform":"linux",
			"restart_policy":"unless-stopped",
			"type":1,
			"volumes":[
				{
					"bind":"/messier/apps/ddclient",
					"container":"/config"
				}
			]
		},
		{
			"title":"NGINX Proxy Manager",
			"description":"Expose your services easily and securely.",
			"logo":"https://raw.githubusercontent.com/univrs-cloud/virgo-apps/main/images/nginx-proxy-manager.png",
			"categories":["Networking"],
			"name":"nginx-proxy-manager",
			"env":[
				{
					"label":"ADMIN_EMAIL",
					"name":"ADMIN_EMAIL",
					"default":""
				},
				{
					"label":"ADMIN_PASSWORD",
					"name":"ADMIN_PASSWORD",
					"default":""
				}
			],
			"platform":"linux",
			"repository":{
				"url":"https://github.com/univrs-cloud/virgo-apps/",
				"stackfile":"stack/nginx-proxy-manager.yaml"
			},
			"type":3
		},
		{
			"title":"Authelia",
			"description":"The Single Sign-On Multi-Factor portal for web apps.",
			"logo":"https://raw.githubusercontent.com/univrs-cloud/virgo-apps/main/images/authelia.png",
			"categories":["System"],
			"name":"authelia",
			"platform":"linux",
			"repository":{
				"url":"https://github.com/univrs-cloud/virgo-apps/",
				"stackfile":"stack/authelia.yaml"
			},
			"type":3
		},
		{
			"title":"Pi-Hole + Unbound",
			"description":"A network-level advertisement and internet tracker blocking application which acts as a DNS sinkhole. This version has Ubound software installed on it so you don't need to rely on external DNS providers.",
			"logo":"https://raw.githubusercontent.com/univrs-cloud/virgo-apps/main/images/pihole.png",
			"categories":["Networking"],
			"env":[
				{
					"label":"DOMAIN_NAME",
					"name":"DOMAIN_NAME",
					"default":"univrs"
				},
				{
					"label":"FTLCONF_LOCAL_IPV4",
					"name":"FTLCONF_LOCAL_IPV4",
					"default":"192.168.100.2"
				},
				{
					"label":"DHCP_ACTIVE",
					"name":"DHCP_ACTIVE",
					"default":"true"
				},
				{
					"label":"DHCP_START",
					"name":"DHCP_START",
					"default":"192.168.100.100"
				},
				{
					"label":"DHCP_END",
					"name":"DHCP_END",
					"default":"192.168.100.250"
				},
				{
					"label":"DHCP_ROUTER",
					"name":"DHCP_ROUTER",
					"default":"192.168.100.1"
				},
				{
					"label":"WEBPASSWORD",
					"name":"WEBPASSWORD",
					"default":"pa$$word"
				}
			],
			"name":"pihole",
			"platform":"linux",
			"repository":{
				"url":"https://github.com/univrs-cloud/virgo-apps/",
				"stackfile":"stack/pihole.yaml"
			},
			"type":3
		},
		{
			"title":"WireGuard",
			"description":"An extremely simple yet fast and modern VPN.",
			"logo":"https://raw.githubusercontent.com/univrs-cloud/virgo-apps/main/images/wireguard.png",
			"categories":["Networking"],
			"env":[
				{
					"description":"DDNS domain",
					"label":"WG_HOST",
					"name":"WG_HOST",
					"default":"example.domain.com"
				},
				{
					"description":"Leave blank to access WebUI without loggin",
					"label":"PASSWORD_HASH",
					"name":"PASSWORD_HASH",
					"default":"ENTER A BCRYPT PASSWORD HASH"
				},
				{
					"label":"WG_DEFAULT_DNS",
					"name":"WG_DEFAULT_DNS",
					"default":"192.168.100.2"
				},
				{
					"label":"WG_DEFAULT_ADDRESS",
					"name":"WG_DEFAULT_ADDRESS",
					"default":"10.6.0.x"
				},
				{
					"label":"WG_ALLOWED_IPS",
					"name":"WG_ALLOWED_IPS",
					"default":"0.0.0.0/0"
				}
			],
			"name":"wg-easy",
			"platform":"linux",
			"repository":{
				"url":"https://github.com/univrs-cloud/virgo-apps/",
				"stackfile":"stack/wireguard-easy.yaml"
			},
			"type":3
		},
		{
			"title":"WeTTY",
			"description":"A terminal in a browser.",
			"logo":"https://raw.githubusercontent.com/univrs-cloud/virgo-apps/main/images/terminal.png",
			"categories":["Networking"],
			"env":[
				{
					"label":"SSH_HOST",
					"name":"SSH_HOST",
					"default":"192.168.100.2"
				},
				{
					"label":"SSH_USER",
					"name":"SSH_USER",
					"default":"voyager"
				},
				{
					"label":"BASE",
					"name":"BASE",
					"default":"/",
					"preset":true
				}
			],
			"image":"ghcr.io/butlerx/wetty:latest",
			"name":"wetty",
			"platform":"linux",
			"restart_policy":"unless-stopped",
			"type":1,
			"ports":[
				"2222:3000/tcp"
			]
		},
		{
			"title":"qBittorrent",
			"description":"A BitTorrent client.",
			"logo":"https://raw.githubusercontent.com/univrs-cloud/virgo-apps/main/images/qbittorrent.png",
			"categories":["Productivity"],
			"env":[
				{
					"label":"PUID",
					"name":"PUID",
					"default":"1000",
					"preset":true
				},
				{
					"label":"PGID",
					"name":"PGID",
					"default":"100",
					"preset":true
				},
				{
					"label":"WEBUI_PORT",
					"name":"WEBUI_PORT",
					"default":"6080",
					"preset":true
				}
			],
			"image":"lscr.io/linuxserver/qbittorrent:latest",
			"name":"qbittorrent",
			"platform":"linux",
			"restart_policy":"unless-stopped",
			"type":1,
			"ports":[
				"6080:6080/tcp",
				"6881:6881/tcp",
				"6881:6881/udp"
			],
			"volumes":[
				{
					"bind":"/messier/apps/qbittorrent",
					"container":"/config"
				},
				{
					"bind":"/downloads",
					"container":"/downloads"
				}
			]
		},
		{
			"title":"NextCloud",
			"description":"A file sharing and collaboration platform that allows users to store, access, and share their data from any device or location.",
			"note":"The host of the database is nextcloud-db:3306. The database user is nextcloud and the database is nextcloud.",
			"logo":"https://raw.githubusercontent.com/univrs-cloud/virgo-apps/main/images/nextcloud.png",
			"categories":["Productivity"],
			"env":[
				{
					"label":"NEXTCLOUD_ADMIN_USER",
					"name":"NEXTCLOUD_ADMIN_USER",
					"default":"voyager"
				},
				{
					"label":"NEXTCLOUD_ADMIN_PASSWORD",
					"name":"NEXTCLOUD_ADMIN_PASSWORD",
					"default":"pa$$word"
				},
				{
					"label":"NEXTCLOUD_TRUSTED_DOMAINS",
					"name":"NEXTCLOUD_TRUSTED_DOMAINS"
				},
				{
					"label":"NEXTCLOUD_DATA_DIR",
					"name":"NEXTCLOUD_DATA_DIR",
					"default":"/nextcloud"
				},
				{
					"label":"MYSQL_PASSWORD",
					"name":"MYSQL_PASSWORD",
					"default":"nextcloud"
				},
				{
					"label":"MYSQL_ROOT_PASSWORD",
					"name":"MYSQL_ROOT_PASSWORD",
					"default":"nextcloud"
				},
				{
					"label":"ONLYOFFICE_JWT_SECRET",
					"name":"ONLYOFFICE_JWT_SECRET"
				}
			],
			"name":"nextcloud",
			"platform":"linux",
			"repository":{
				"url":"https://github.com/univrs-cloud/virgo-apps/",
				"stackfile":"stack/nextcloud.yaml"
			},
			"type":3
		},
		{
			"title":"Penpot",
			"description":"A design & prototyping tool for product teams.",
			"logo":"https://raw.githubusercontent.com/univrs-cloud/virgo-apps/main/images/penpot.png",
			"categories":["Productivity"],
			"env":[
				{
					"label":"PUBLIC_URI",
					"name":"PUBLIC_URI",
					"default":"http://ip:5080"
				},
				{
					"label":"POSTGRES_PASSWORD",
					"name":"POSTGRES_PASSWORD",
					"default":"penpot"
				}
			],
			"name":"penpot",
			"platform":"linux",
			"repository":{
				"url":"https://github.com/univrs-cloud/virgo-apps/",
				"stackfile":"stack/penpot.yaml"
			},
			"type":3
		},
		{
			"title":"OneDev",
			"description":"Git hosting, code review, team collaboration, package registry and CI/CD",
			"logo":"https://raw.githubusercontent.com/univrs-cloud/virgo-apps/main/images/onedev.png",
			"categories":["Productivity"],
			"env":[
				{
					"label":"PUID",
					"name":"PUID",
					"default":"1000",
					"preset":true
				},
				{
					"label":"PGID",
					"name":"PGID",
					"default":"100",
					"preset":true
				}
			],
			"image":"1dev/server:latest",
			"name":"onedev",
			"platform":"linux",
			"restart_policy":"unless-stopped",
			"type":1,
			"ports": [
				"6610:6610/tcp",
				"6611:6611/tcp"
			],
			"volumes":[
				{
					"bind": "/messier/apps/onedev",
					"container": "/opt/onedev"
				},
				{
					"bind":"/var/run/docker.sock",
					"container":"/var/run/docker.sock",
					"readonly":true
				}
			]
		},
		{
			"title":"Watchtower",
			"description":"Automatically updates your apps to the latest version.",
			"logo":"https://raw.githubusercontent.com/univrs-cloud/virgo-apps/main/images/watchtower.png",
			"categories":["System"],
			"image":"ghcr.io/containrrr/watchtower:latest",
			"command": "--cleanup",
			"name":"watchtower",
			"platform":"linux",
			"restart_policy":"unless-stopped",
			"type":1,
			"volumes":[
				{
					"bind":"/var/run/docker.sock",
					"container":"/var/run/docker.sock",
					"readonly":true
				}
			]
		}
	]
}
